
Resources:
tosca_name -> resource_name

Models:
SupportsConcurrentCommands = true


Family:
A combination of
   cloudshell.capabilities.CloudshellType
   +
   cloudshell.capabilities.CloudShellEndPoint


Attribute mapping: cloudshell.capabilities.FamilyType
---------------------------------------------------------
locked_by_default:
  default_share_level: exclusive -> true, else -> false

admin_only:
  permissions: admin_only -> true, else -> false

connectable: (default) false

act_as_group: (default) false

is_service: (default) false

resource_type: (default) physical

is_mappable: (default) false

is_mappable_container: (default) false

is_all_connected_container: (default) false

allow_multiple_connections: (default) false

supports_multicast_mappings: (default) false

supports_loopback_mappings: (default) false

is_power_switch: (default) false

is_console_server: (default) false

is_license_check_required: (default) false



Attribute mapping: cloudshell.capabilities.CloudShellEndPoint
---------------------------------------------------------
connectable = false




tags: [t1,t2,t3...]


# tags list [translation to CloudShell]:

configuration = [Configuration == true]
setting = [Setting == true]
not_searchable = [Displayed In Search Filters == false]
abstract_filter = [Available For Abstract Resources == true]
include_in_insight = [Displayed In insight == true]
readonly_to_users = [Admin Only == true]
display_in_diagram = [Displayed In Diagram == true]
connection_attribute = [Link Configuration == true]
read_only

